{
  "$schema": "vscode://schemas/color-theme",
  "type": "dark",
  "name": "Gentle Mary",
  "tokenColors": [
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#CCB385"
      }
    },
    {
      "name": "Boolean",
      "scope": "constant.language.boolean",
      "settings": {
        "foreground": "#88CCCC"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#88CCCC"
      }
    },
    {
      "name": "Identifier",
      "scope": "variable, support.variable, support.class, support.constant, meta.definition.variable, entity.name.function",
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword, modifier, variable.language.this, support.type.object, constant.language",
      "settings": {
        "foreground": "#AF4A67",
        "fontStyle": "bold"
      }
    },
    {
      // This is a departure from the WebStorm theme
      "name": "Keyword",
      "scope": "variable.language.this",
      "settings": {
        "foreground": "#A576A0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Brackets",
      "scope": "meta.brace",
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    {
      "name": "General punctuation",
      "scope": "punctuation.separator.key-value.js, punctuation.definition.block",
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    {
      "name": "Function call",
      "scope": "entity.name.function, support.function",
      "settings": {
        "foreground": "#3DA676"
      }
    },
    {
      "name": "Storage",
      "scope": "storage.type, storage.modifier",
      "settings": {
        "foreground": "#AF4A67",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Modules",
      "scope": "support.module, support.node",
      "settings": {
        "foreground": "#CCCCCC",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Type",
      "scope": "support.type, support.class",
      "settings": {
        "foreground": "#A576A0"
      }
    },
    {
      "name": "Type",
      "scope": "entity.name.type, entity.other.inherited-class",
      "settings": {
        "foreground": "#A576A0"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#777777"
      }
    },
    {
      "name": "Class",
      "scope": "entity.name.type.class",
      "settings": {
        "foreground": "#3DA676"
      }
    },
    {
      "name": "Class variable",
      "scope": "variable.object.property, variable.other.property",
      "settings": {
        "foreground": "#4B95DC"
      }
    },
    {
      "name": "Class method",
      "scope": "meta.definition.method",
      "settings": {
        "foreground": "#3DA676"
      }
    },
    {
      "name": "Function definition",
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#3DA676"
      }
    },
    {
      "name": "Template expression",
      "scope": "template.expression.begin, template.expression.end",
      "settings": {
        "foreground": "#AF4A67",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Template expression interpolation",
      "scope": "punctuation.definition.template-expression.*.*, punctuation.definition.template-expression",
      "settings": {
        "foreground": "#AF4A67",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Operator",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#CCCCCC",
        "fontStyle": ""
      }
    },
    {
      "name": "JavaScript semicolon",
      "scope": "punctuation.terminator.statement",
      "settings": {
        "foreground": "#8A8A8A"
      }
    },
    {
      "name": "TypeScript type definition",
      "scope": "keyword.operator.type.annotation.ts",
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    {
      "name": "YAML key",
      "scope": "entity.name.tag.yaml",
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    {
      "name": "Modifier",
      "scope": "modifier",
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    {
      "name": "JSON key",
      "scope": "meta.object-literal.key, meta.object-literal.key string, support.type.property-name.json",
      "settings": {
        "foreground": "#4B95DC"
      }
    },
    {
      "name": "JSON constant",
      "scope": "constant.language.json",
      "settings": {
        "foreground": "#AF4A67",
        "fontStyle": "bold"
      }
    },
    {
      "name": "CSS class",
      "scope": "entity.other.attribute-name.class",
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    {
      "name": "CSS ID",
      "scope": "entity.other.attribute-name.id",
      "settings": {
        "foreground": "#3DA676"
      }
    },
    {
      "name": "CSS tag",
      "scope": "source.css entity.name.tag",
      "settings": {
        "foreground": "#A576A0"
      }
    },
    {
      "name": "HTML tag outer",
      "scope": "meta.tag, punctuation.definition.tag",
      "settings": {
        "foreground": "#A576A0"
      }
    },
    {
      "name": "HTML tag inner",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#A576A0"
      }
    },
    {
      "name": "HTML tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#3DA676"
      }
    },
    {
      "name": "HTML tag attribute punctuation",
      "scope": "meta.tag.any, meta.tag.inline.any",
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    {
      "name": "HTML tag attribute punctuation (special cases)",
      "scope": "meta.tag.metadata.script.html, meta.tag.structure.any.html",
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    {
      "name": "HTML content",
      "scope": "text.html.basic, meta.jsx.children.js",
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    {
      "name": "JSX tag attribute equals",
      "scope": "meta.tag.attribute keyword.operator.assignment",
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    {
      "name": "JSX tag attribute curly brackets",
      "scope": "punctuation.section.embedded",
      "settings": {
        //"foreground": "#CCCCCC"
        "foreground": "#999999",
        //"fontStyle": "bold"
      }
    },
    {
      "name": "Markdown heading",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#3DA676",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown link title",
      "scope": "string.other.link.title.markdown",
      "settings": {
        "foreground": "#A576A0"
      }
    },
    {
      "name": "Markdown link punctuation",
      "scope": "punctuation.definition.string.begin.markdown, punctuation.definition.string.end.markdown, punctuation.definition.metadata.markdown",
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    {
      "name": "Markdown link url",
      "scope": "text.html.markdown meta.link.inline, meta.link.reference",
      "settings": {
        "foreground": "#4B95DC"
      }
    },
    {
      "name": "Markdown list item",
      "scope": "text.html.markdown beginning.punctuation.definition.list",
      "settings": {
        "foreground": "#AF4A67",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown italic",
      "scope": "markup.italic",
      "settings": {
        "foreground": "#CCCCCC",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown bold",
      "scope": "markup.bold",
      "settings": {
        "foreground": "#CCCCCC",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown bold italic",
      "scope": "markup.bold markup.italic, markup.italic markup.bold",
      "settings": {
        "foreground": "#CCCCCC",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Markdown inline code",
      "scope": "markup.inline.raw.string.markdown",
      "settings": {
        "background": "#000000",
        "foreground": "#777777"
      }
    },
    {
      "name": "Markdown code block",
      "scope": "markup.fenced_code.block.markdown",
      "settings": {
        "background": "#000000",
        "foreground": "#777777"
      }
    },
    {
      "name": "INI property name",
      "scope": "keyword.other.definition.ini",
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    {
      "name": "INI section title",
      "scope": "entity.name.section.group-title.ini",
      "settings": {
        "foreground": "#AF4A67",
        "fontStyle": "bold"
      }
    },
    {
      "name": "C# class",
      "scope": "source.cs meta.class.identifier storage.type",
      "settings": {
        "foreground": "#3DA676"
      }
    },
    {
      "name": "C# class method",
      "scope": "source.cs meta.method.identifier entity.name.function",
      "settings": {
        "foreground": "#4B95DC"
      }
    },
    {
      "name": "C# function call",
      "scope": "source.cs meta.method-call meta.method, source.cs entity.name.function",
      "settings": {
        "foreground": "#3DA676"
      }
    },
    {
      "name": "C# type",
      "scope": "source.cs storage.type",
      "settings": {
        "foreground": "#A576A0"
      }
    },
    {
      "name": "C# return type",
      "scope": "source.cs meta.method.return-type",
      "settings": {
        "foreground": "#A576A0"
      }
    },
    {
      "name": "C# preprocessor",
      "scope": "source.cs meta.preprocessor",
      "settings": {
        "foreground": "#777777"
      }
    },
    {
      "name": "C# namespace",
      "scope": "source.cs entity.name.type.namespace",
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    {
      "name": "shell builtin",
      "scope": "source.shell support.function.builtin.shell",
      "settings": {
        "foreground": "#AF4A67"
      }
    },
    // The following shell colours were written for Jeff Hyklin's "Better Shell Syntax" not the default syntax scopes (which were limited in October 2019)
    // Although even this fails to highlight variable expansion inside strings, and option arguments after a \ line escape.
    {
      "name": "shell command",
      "scope": "source.shell entity.name.command.shell",
      "settings": {
        "foreground": "#3DA676"
      }
    },
    {
      "name": "shell argument",
      "scope": "source.shell string.unquoted.argument.shell",
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    {
      "name": "shell option",
      "scope": "source.shell constant.other.option.shell",
      "settings": {
        "foreground": "#4B95DC"
      }
    },
    {
      "name": "shell variable",
      "scope": "source.shell variable.other.assignment.shell, source.shell variable.other.normal.shell",
      "settings": {
        "foreground": "#88CCCC"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#6796e6"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#cd9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#f44747"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#b267e6"
      }
    },
    // Highlight where a variable is defined
    // (I actually want to do this for variable assignments too, but I found reads were indistinguishable from writes)
    /*
    {
      "name": "Variable definition",
      "scope": "meta.definition.variable.js variable.other.readwrite.js, meta.definition.variable.js variable.other.constant.js",
      "settings": {
        "foreground": "#e4e4e4"
      }
    },
    */
  ],
